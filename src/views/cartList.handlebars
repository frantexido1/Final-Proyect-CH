<div class="container">
  <h1>Productos en el Carrito</h1>
  <div class="product-list">
    {{#each cart}}
      <div class="product-card">
        <img src="{{this.thumbnail}}" alt="{{this.title}} Thumbnail" />
        <h2>{{this.title}}</h2>
        <p>Precio: ${{this.price}}</p>
      </div>
    {{/each}}
  </div>
</div>